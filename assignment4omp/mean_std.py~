import numpy as np


for i in [1, 2, 4, 8, 12, 16, 24, 32, 48]:
    times = []
    mflops = []
    filename = jobname = 'job_' + sys.argv[1] + '_' + str(i) + '.out'
    with open(filename, 'r') as f:
        for l in enumerate(f):
            if 'megaflops' in l:
                mflops.append(float(l.split(':')[-1].strip()))
            elif 'Execution time' in l:
                times.append(float(l.split(':')[-1].strip()))
        print 'Threads: {} -> \n Time mean: {} / std: {}\n Flops mean: {} std: {}\n\n'.\
            format(i+1, np.mean(times), np.std(times), np.mean(flops), np.std(flops))

